    "gatsby-source-filesystem": "^2.0.38",
    "gatsby-source-graphql": "^2.0.18",
    "gatsby-transformer-json": "^2.1.1",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.21",
    "modern-normalize": "^1.0.0",
    "normalize.css": "^8.0.0",
    "polished": "^4.0.5",
    "prism-themes": "^1.0.1",
    "prismjs": "^1.15.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-spring": "^8.0.20",
    "typescript": "^4.0.2"
