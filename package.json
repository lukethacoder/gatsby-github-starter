{
  "name": "gatsby-github-starter",
  "description": "GatsbyJS starter feat. Github GraphQL v4 / TypeScript / Emotion / React Spring",
  "version": "1.0.0",
  "author": "Luke Secomb <hello@lukesecomb.digital>",
  "keywords": [
    "gatsby",
    "github",
    "graphql"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.{ts,tsx,md}\"",
    "lint": "tslint 'src/**/*.{ts,tsx}' -p tsconfig.json",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@emotion/core": "^11.0.0",
    "@emotion/styled": "^10.0.11",
    "babel-plugin-styled-components": "^1.7.1",
    "classnames": "^2.2.6",
    "gatsby": "^2.8.2",
    "gatsby-image": "^2.1.2",
    "gatsby-plugin-canonical-urls": "^3.0.0",
    "gatsby-plugin-emotion": "^6.0.0",
    "gatsby-plugin-google-analytics": "^2.0.20",
    "gatsby-plugin-manifest": "^2.1.1",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-plugin-typescript": "^2.0.15",
    "gatsby-remark-copy-linked-files": "^2.0.13",
    "gatsby-remark-images": "^3.1.7",
    "gatsby-remark-prismjs": "^3.2.9",
    "gatsby-remark-responsive-iframe": "^2.0.5",
