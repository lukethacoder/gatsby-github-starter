    "@types/classnames": "^2.2.8",
    "@types/node": "^14.0.24",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "gh-pages": "^3.1.0",
    "prettier": "^2.1.1",
    "rimraf": "^3.0.0",
    "tslint": "^6.1.3",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
